{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:vue/essential",
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "semi": 0,
        "comma-dangle": 0,
        "quotes": 0,
        "eol-last": 0,
        "indent": 0,
        "space-before-function-paren": 0
    }
}